<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="TestNG" parallel="methods" thread-count="3">

    <test name="Example Test" preserve-order="false">
        <classes>
            <class name="LoginTests">
                <methods>
                    <include name="loginValidEmailPassword"/>
                    <include name="loginInvalidEmailValidPassword"/>
                    <include name="loginValidEmailEmptyPassword"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>